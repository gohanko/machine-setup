{
    "policies": {
        "DNSOverHTTPS": {
            "Enabled": true
        },
        "SearchEngines": {
            "Default": "DuckDuckGo"
        },
        "SearchSuggestEnabled": false,
        "EnableTrackingProtection": {
            "Value": true,
            "Cryptomining": true,
            "Fingerprinting": true
        },
        "DisablePocket": true,
        "Cookies": {
            "Default": true,
            "AcceptThirdParty": "never",
            "ExpireAtSessionEnd": false,
            "RejectTracker": true
        },
        "OfferToSaveLogins": true,
        "DisableTelemetry": true,
        "DisableFirefoxStudies": true,
        "Extensions": {
            "Install": [
                "https://addons.mozilla.org/firefox/downloads/file/3814538/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/3682334/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/3716273/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/3837362/latest.xpi"
            ]
        },
        "Preferences": {
            "dom.security.https_only_mode": {
                "Value": true,
                "Status": "user"
            },
            "privacy.firstparty.isolate": {
                "Value": true,
                "Status": "user"
            },
            "privacy.donottrackheader.enabled": {
                "Value": true,
                "Status": "user"
            },
            "browser.toolbars.bookmarks.visibility": {
                "Value": "never",
                "Status": "user"
            }
        }
    }
}